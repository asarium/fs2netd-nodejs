{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "max-classes-per-file": []
  },
  "rulesDirectory": []
}