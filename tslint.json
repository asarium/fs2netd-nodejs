{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case",
      "allow-snake-case"
    ],
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "max-classes-per-file": [],
    "no-trailing-whitespace": false,
    "max-line-length": [
      false
    ]
  },
  "rulesDirectory": []
}